{
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "@docucraft/srender",
		"declarationDir": "dist",

		"target": "ESNext",

		"module": "ESNext",
		"moduleResolution": "node",

		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src", "rollup.config.js"],
	"exclude": ["node_modules"]
}
